

<%= form_with model: @admin,url: new_admin_session_path do |f| %>
<div class="container">
 <div>
   <h3>管理者ログイン</h3>
  <div class="form-group">
    <%= f.label :メールアドレス %>
    <%= f.email_field :email,class: "form-control", autofocus: true, autocomplete: "email" %>
  </div>

  <div class="form-group">
    <%= f.label :パスワード %>
    <%= f.password_field :password,class: "form-control", autocomplete: "current-password" %>
  </div>

  <div class="actions">
    <%= f.submit "ログイン" %>
  </div>
 </div>
</div>
<% end %>

<%= render "admin/shared/links" %>
